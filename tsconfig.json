{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "dom.iterable",
      "scripthost"
    ],
    "allowJs": false,
    "newLine": "LF",
    "jsx": "react",
    "jsxFactory": "NFJSX.createElement",
    "sourceMap": true,
    "removeComments": true,
    "noEmitOnError": true,
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "baseUrl": "./src",
    "strictPropertyInitialization": false,
    "paths": {
      "canvas": ["ts/components/graph/canvas"],
      "node": ["ts/components/graph/node"],
      "library": ["ts/components/objects/library"],
      "window": ["ts/components/ui/window"],
      "button": ["ts/components/ui/button"],
      "datas": ["ts/utils/datas"],
      "dom": ["ts/utils/dom"],
      "jsx": ["ts/utils/jsx"],
      "windows-manager": ["ts/utils/windows-manager"]
    }
  },
  "include":[
    "src"
  ]
}