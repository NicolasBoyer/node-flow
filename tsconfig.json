{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "dom.iterable",
      "scripthost"
    ],
    "allowJs": false,
    "newLine": "LF",
    "jsx": "react",
    "sourceMap": true,
    "removeComments": true,
    "noEmitOnError": true,
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true
  },
  "include":[
    "src"
  ]
}